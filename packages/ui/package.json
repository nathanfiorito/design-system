{
    "name": "@ds/ui",
    "version": "0.0.1",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "sideEffects": [
        "**/*.css"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./foundations": {
            "types": "./dist/foundations/index.d.ts",
            "default": "./dist/foundations/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json && cp src/foundations/*.css dist/foundations/"
    },
    "devDependencies": {
        "typescript": "^5.0.0"
    }
}